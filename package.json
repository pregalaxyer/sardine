{
  "name": "sardine",
  "version": "0.0.0",
  "description": "a mock serve tool from swagger",
  "keywords": [],
  "main": "dist/sardine.umd.js",
  "module": "dist/sardine.es.js",
  "typings": "dist/types/sardine.d.ts",
  "files": [
    "dist"
  ],
  "author": "dylan <dylan_zeng92@qq.com>",
  "repository": "https://github.com/diveDylan/sardine",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'src/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": " rollup -c rollup.config.ts ",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:cov": "jest --coverage --no-cache --detectOpenHandles --forceExit",
    "codecov": "codecov --token=c2eba3bd-e6e1-4a08-b226-fe0829dcd151",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "init": "semantic-release setup"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/chance": "^1.1.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^16.3.0",
    "@types/node-fetch": "^2.5.10",
    "babel-jest": "^27.0.6",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.45.2",
    "semantic-release": "^17.4.4",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@types/koa": "^2.13.4",
    "chance": "^1.1.7",
    "codecov": "^3.8.2",
    "koa": "^2.13.1",
    "node-fetch": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0"
  }
}
